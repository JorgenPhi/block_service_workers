{
  "manifest_version": 2,
  "name": "Block Service Workers",
  "description": "Disallow to register Service Workers",
  "version": "0.0.2",
  "permissions":[
    "tabs",
    "storage"
  ],
  "background":
  {
    "scripts": ["confirm.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_start",
      "js": ["index.js"]
    }
  ]
}